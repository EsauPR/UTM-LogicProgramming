  <pelis:guionista>frank darabont</pelis:guionista>
  <pelis:actor_principal>tim robbins</pelis:actor_principal>
  <pelis:actores_reparto>morgan freeman, bob gunton, james whitmore, gil bellows, william sadler, mark rolston, clancy brown, david proval, jeffrey demunn, jude ciccolella, don mcmanus, alfonso freeman</pelis:actores_reparto>
